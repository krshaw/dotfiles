execute pathogen#infect()
syntax on
filetype plugin indent on
colorscheme gruvbox
set background=dark
set number
set autoindent
noremap  <Up> ""
noremap! <Up> <Esc>
noremap  <Down> ""
noremap! <Down> <Esc>
noremap  <Left> ""
noremap! <Left> <Esc>
noremap  <Right> ""
noremap! <Right> <Esc>
set number relativenumber
set nu rnu
inoremap {<CR> {<CR>}<Esc>ko
